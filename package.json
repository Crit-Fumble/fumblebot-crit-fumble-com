{
  "name": "fumblebot",
  "description": "A Discord bot for TTRPG Gaming based on the boilerplate by Nicolas Ribeiro",
  "version": "0.0.1",
  "private": false,
  "main": "./src/Launcher.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Crit-Fumble/FumbleBot.git"
  },
  "bugs": {
    "url": "https://github.com/Crit-Fumble/FumbleBot/issues"
  },
  "homepage": "https://github.com/Crit-Fumble/FumbleBot#readme",
  "scripts": {
    "build": "cd src && npx babel ./ --out-dir ../dist --source-maps inline",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint src --fix",
    "develop": "cd src && node --max_old_space_size=512 --optimize_for_size --expose-gc --gc-interval=1 Launcher.js",
    "start": "cd src && node --max_old_space_size=512 --optimize_for_size --expose-gc --gc-interval=1 Launcher.js",
    "start:do": "node --max_old_space_size=512 --optimize_for_size --expose-gc --gc-interval=1 Launcher.js",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up"
  },
  "license": "MIT",
  "engines": {
    "node": "16.20.2"
  },
  "author": "Crit Fumble Gaming <crit.fumble.web@gmail.com>",
  "dependencies": {
    "@babel/register": "^7.22.5",
    "@napi-rs/canvas": "^0.1.41",
    "chrono-node": "^2.6.5",
    "discord.js": "14.13.0",
    "dotenv": "16.0.1",
    "gpt-3-encoder": "^1.1.4",
    "openai": "^3.3.0",
    "vite": "^4.4.9",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/node": "^7.22.10",
    "@babel/preset-env": "^7.22.14",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.2"
  }
}
